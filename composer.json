{
    "require": {
        "cakephp/chronos": "^2.0",
        "lcobucci/jwt": "^4.1",
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^3.9",
        "selective/basepath": "^2.1",
        "selective/config": "^1.1",
        "slim/http": "^1.0",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
